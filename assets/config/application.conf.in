<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <application>
        <name>${PROJECT}</name>
        <version>${PROJECT_VERSION}</version>
        <url>${ENDPOINT}</url>
    </application>
    <oauth>
        <clientid>${CLIENTID}</clientid>
        <scope>read</scope>
        <secret>${SECRETID}</secret>
        <endpoints>
            <authorize>${ENDPOINT}/${APIPATH}/oauth/authorize</authorize>
            <token>${ENDPOINT}/${APIPATH}/oauth/token</token>
        </endpoints>
    </oauth>
    <data>
        <endpoints>
            <chip>${ENDPOINT}/${APIPATH}/rest/chip</chip>
            <dataset>${ENDPOINT}/${APIPATH}/rest/dataset</dataset>
            <features>${ENDPOINT}/${APIPATH}/rest/features</features>
            <selection>${ENDPOINT}/${APIPATH}/rest/selection</selection>
            <user>${ENDPOINT}/${APIPATH}/rest/account/current/user</user>
            <imagealignment>${ENDPOINT}/${APIPATH}/rest/imagealignment</imagealignment>
            <figure>${ENDPOINT}/${APIPATH}/rest/image/compressed</figure>
            <version>${ENDPOINT}/${APIPATH}/rest/versionsupportinfo</version>
        </endpoints>
    </data>
</configuration>
